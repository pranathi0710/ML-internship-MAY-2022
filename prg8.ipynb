    "def function(li):\n",
    "    li.sort()\n",
    "    rev=li\n",
    "    rev.reverse()\n",
    "    pairs=0\n",
    "    i=0\n",
    "    while(i < len(li)):\n",
    "        t=li.count(li[i])\n",
    "        pairs=pairs+t//2\n",
    "        i=i+t\n",
    "    return pairs\n",
    "li=[1,2,1,2,1,3,2]\n",
    "print(function(li))\n",
    "        \n"
   ]
  }

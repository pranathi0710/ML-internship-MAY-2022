   "source": [
    "def factorial(n):\n",
    "    if n==1:\n",
    "        return 1\n",
    "    else:\n",
    "        return n*factorial(n-1)\n",
    "n=int(input(\"Enter a number: \"))\n",
    "res=1\n",
    "for i in range(1,n+1):\n",
    "    res=res*factorial(i)\n",
    "print(res)\n"
   ]
  }
